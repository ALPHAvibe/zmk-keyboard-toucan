#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

#define BASE 0
#define MSC 1
#define SYM 2
#define ADJ 3
#define MSE 4
#define NUML 5
#define NUMR 6
#define NAV 7
#define FNCL 8
#define FNCR 9
#define MISC 10
#define GAME 11

&lt {
    tapping-term-ms = <250>;
    quick-tap-ms = <220>;
    flavor = "balanced";
};

&mt {
    tapping-term-ms = <275>;
    quick-tap-ms = <275>;
    require-prior-idle-ms = <100>;
    flavor = "balanced";
};

/ {
    conditional_layers {
        compatible = "zmk,conditional-layers";

        NAV {
            if-layers = <5 6>;
            then-layer = <7>;
        };

        MISC {
            if-layers = <8 9>;
            then-layer = <10>;
        };
    };

    macros {
        nav_back: nav_back {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(LC(LS(LEFT_BRACKET)))>;
            label = "NAV_BACK";
        };

        nav_forward: nav_forward {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(LC(LS(RIGHT_BRACKET)))>;
            label = "NAV_FORWARD";
        };

        nav_prev_tab: nav_prev_tab {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(LS(LA(LEFT_BRACKET)))>;
            label = "NAV_PREV_TAB";
        };

        nav_next_tab: nav_next_tab {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(LS(LA(RIGHT_BRACKET)))>;
            label = "NAV_NEXT_TAB";
        };
    };

    behaviors {
        hml: hml {
            compatible = "zmk,behavior-hold-tap";
            label = "HOME_ROW_MODS_LEFT";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            hold-trigger-key-positions = <6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 40 41 39>;
            tapping-term-ms = <250>;
            quick-tap-ms = <220>;
            require-prior-idle-ms = <100>;
            flavor = "balanced";
            hold-trigger-on-release;
        };

        hmr: hmr {
            compatible = "zmk,behavior-hold-tap";
            label = "HOME_ROW_MODS_RIGHT";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            tapping-term-ms = <250>;
            quick-tap-ms = <220>;
            require-prior-idle-ms = <100>;
            flavor = "balanced";
            hold-trigger-on-release;
            hold-trigger-key-positions = <0 1 2 3 4 5 12 13 14 15 16 17 24 25 26 27 28 29 36 37 38>;
        };

        hnpt: hnpt {
            compatible = "zmk,behavior-hold-tap";
            label = "HNPT";
            bindings = <&kp>, <&nav_prev_tab>;

            #binding-cells = <2>;
            tapping-term-ms = <275>;
            quick-tap-ms = <275>;
            require-prior-idle-ms = <100>;
            flavor = "balanced";
            hold-trigger-key-positions = <6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 39 40 41>;
            hold-trigger-on-release;
        };

        hnnt: hnnt {
            compatible = "zmk,behavior-hold-tap";
            label = "HNNT";
            bindings = <&kp>, <&nav_next_tab>;

            #binding-cells = <2>;
            tapping-term-ms = <275>;
            quick-tap-ms = <275>;
            require-prior-idle-ms = <100>;
            flavor = "balanced";
            hold-trigger-key-positions = <6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 39 40 41>;
            hold-trigger-on-release;
        };

        hnb: hnb {
            compatible = "zmk,behavior-hold-tap";
            label = "HNB";
            bindings = <&kp>, <&nav_back>;

            #binding-cells = <2>;
            tapping-term-ms = <275>;
            quick-tap-ms = <275>;
            require-prior-idle-ms = <100>;
            flavor = "balanced";
            hold-trigger-key-positions = <6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 39 40 41>;
            hold-trigger-on-release;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        base {
            display-name = "BASE";
            bindings = <
&kp ESCAPE  &kp Q  &kp W  &kp E  &kp R  &kp T  &kp Y  &kp U  &kp I  &kp O  &kp P  &kp BSPC  &kp TAB  &hml LEFT_SHIFT A  &hml LEFT_CONTROL S  &hml LEFT_ALT D  &hml LEFT_META F  &lt 4 G  &lt 4 H  &hmr RIGHT_META J  &hmr RIGHT_ALT K  &hmr RIGHT_CONTROL L  &hmr RIGHT_SHIFT SEMICOLON  &kp SQT  &lt 8 LEFT_SHIFT  &lt 8 Z  &kp X  &kp C  &kp V  &lt 1 B  &lt 1 N  &kp M  &kp COMMA  &kp DOT  &lt 9 SLASH  &lt 9 RIGHT_SHIFT  &mt LS(LA(LEFT_CONTROL)) ESCAPE  &lt 5 BACKSPACE  &lt 2 DELETE  &lt 2 ENTER  &lt 6 SPACE  &mt RS(RA(RC(RIGHT_META))) TAB
            >;
        };

        msc {
            display-name = "MSC";
            bindings = <
&kp TAB  &kp N1  &kp N2  &kp N3  &kp N4  &kp N5  &kp N6  &kp N7  &kp N8  &kp N9  &kp N0  &kp BSPC  &bt BT_CLR  &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &kp LEFT  &kp DOWN  &kp UP  &kp RIGHT  &trans  &trans  &kp LSHFT  &studio_unlock  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &mkp RCLK  &mkp MCLK  &mkp LCLK  &mkp LCLK  &mkp MCLK  &mkp RCLK
            >;
        };

        sym {
            display-name = "SYM";
            bindings = <
&trans  &kp EXCL  &kp AT  &kp HASH  &kp DLLR  &kp PRCNT  &kp CARET  &kp AMPS  &kp ASTERISK  &kp LPAR  &kp RPAR  &trans  &trans  &kp GRAVE  &kp LEFT_BRACE  &kp LEFT_PARENTHESIS  &kp SQT  &kp UNDERSCORE  &kp MINUS  &kp EQUAL  &kp RIGHT_PARENTHESIS  &kp RIGHT_BRACE  &kp PIPE  &trans  &trans  &kp TILDE  &kp LESS_THAN  &kp LEFT_BRACKET  &kp DOUBLE_QUOTES  &none  &none  &kp PLUS  &kp RIGHT_BRACKET  &kp GREATER_THAN  &kp BACKSLASH  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        adj {
            display-name = "ADJ";
            bindings = <
&kp TAB  &trans  &kp F7  &kp F8  &kp F9  &kp F12  &kp C_VOL_DN  &kp C_MUTE  &kp C_VOL_UP  &trans  &trans  &kp BSPC  &kp LCTRL  &trans  &kp F4  &kp F5  &kp F6  &kp F11  &trans  &trans  &trans  &trans  &trans  &trans  &kp LSHFT  &trans  &kp F1  &kp F2  &kp F3  &kp F10  &trans  &trans  &trans  &trans  &trans  &trans  &kp LGUI  &trans  &kp SPACE  &kp RET  &mo 2  &kp RALT
            >;
        };

        mse {
            display-name = "MSE";
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &mkp RCLK  &mkp MCLK  &mkp LCLK  &mkp LCLK  &mkp MCLK  &mkp RCLK
            >;
        };

        numl {
            display-name = "NUML";
            bindings = <
&trans  &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &kp NUMBER_4  &kp NUMBER_5  &kp NUMBER_6  &kp NUMBER_7  &kp NUMBER_8  &kp NUMBER_9  &kp NUMBER_0  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        numr {
            display-name = "NUMR";
            bindings = <
&trans  &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &kp NUMBER_4  &kp NUMBER_5  &kp NUMBER_6  &kp NUMBER_7  &kp NUMBER_8  &kp NUMBER_9  &kp NUMBER_0  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        nav {
            display-name = "NAV";
            bindings = <
&trans  &kp LS(LA(LC(LG(NUMBER_1))))  &kp LS(LA(LC(LG(NUMBER_2))))  &kp LS(LA(LC(LG(NUMBER_3))))  &kp LS(LA(LC(LG(NUMBER_4))))  &kp LS(LA(LC(LG(NUMBER_5))))  &kp LS(LA(LC(LG(NUMBER_6))))  &kp LA(LS(LC(LG(NUMBER_7))))  &kp LS(LA(LC(LG(NUMBER_8))))  &kp LS(LA(LC(LG(NUMBER_9))))  &kp LS(LA(LC(LG(NUMBER_0))))  &trans  &trans  &kp LEFT_SHIFT  &hnb LEFT_CONTROL 0  &hnpt LEFT_ALT 0  &hnnt LEFT_META 0  &nav_forward  &kp LEFT_ARROW  &kp DOWN_ARROW  &kp UP_ARROW  &kp RIGHT_ARROW  &kp RIGHT_SHIFT  &trans  &trans  &kp LS(LA(LC(Z)))  &kp LS(LA(LC(X)))  &kp LS(LA(LC(C)))  &kp LS(LA(LC(V)))  &kp LS(LA(LC(B)))  &kp HOME  &kp PAGE_DOWN  &kp PAGE_DOWN  &kp END  &kp LS(LA(LC(SLASH)))  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        fncl {
            display-name = "FNCL";
            bindings = <
&none  &kp F1  &kp F2  &kp F3  &kp F4  &kp F5  &kp F6  &kp F7  &kp F8  &kp F9  &kp F10  &none  &none  &none  &kp C_MUTE  &kp C_VOL_DN  &kp C_VOLUME_UP  &kp F11  &kp F12  &kp C_PP  &kp C_PREV  &kp C_NEXT  &none  &none  &trans  &trans  &none  &kp C_BRI_DEC  &kp C_BRIGHTNESS_INC  &trans  &none  &none  &none  &none  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        fncr {
            display-name = "FNCR";
            bindings = <
&none  &kp F1  &kp F2  &kp F3  &kp F4  &kp F5  &kp F6  &kp F7  &kp F8  &kp F9  &kp F10  &none  &none  &none  &kp C_MUTE  &kp C_VOL_DN  &kp C_VOLUME_UP  &kp F11  &kp F12  &kp C_PP  &kp C_PREV  &kp C_NEXT  &none  &none  &trans  &trans  &none  &kp C_BRI_DEC  &kp C_BRIGHTNESS_INC  &trans  &none  &none  &none  &none  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        misc {
            display-name = "MISC";
            bindings = <
&none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &none  &bt BT_CLR  &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &none  &trans  &trans  &kp CAPSLOCK  &none  &none  &none  &none  &none  &none  &none  &trans  &trans  &trans  &to 0  &trans  &trans  &to 11  &trans
            >;
        };

        game {
            display-name = "GAME";
            bindings = <
&kp ESCAPE  &kp Q  &kp W  &kp E  &kp R  &kp T  &kp Y  &kp U  &kp I  &kp O  &kp P  &kp BSPC  &kp TAB  &kp A  &kp S  &kp D  &kp F  &kp G  &lt 4 H  &hmr RIGHT_META J  &hmr RIGHT_ALT K  &hmr RIGHT_CONTROL L  &hmr RIGHT_SHIFT SEMICOLON  &kp SQT  &lt 8 LEFT_SHIFT  &kp Z  &kp X  &kp C  &kp V  &kp B  &lt 1 N  &kp M  &kp COMMA  &kp DOT  &lt 9 SLASH  &lt 9 RIGHT_SHIFT  &mt LS(LA(LEFT_CONTROL)) ESCAPE  &lt 5 BACKSPACE  &lt 2 DELETE  &lt 2 ENTER  &lt 6 SPACE  &mt RS(RA(RC(RIGHT_META))) TAB
            >;
        };
    };
};
